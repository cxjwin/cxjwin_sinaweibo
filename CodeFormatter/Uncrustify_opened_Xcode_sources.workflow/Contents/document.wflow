				<key>ActionName</key>
				<string>Exécuter un script AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>try	if isApplicationRunning("Xcode") then				-- check that uncrustify is correctly installed :		if not (isUncrustifyCorrectlyInstalled()) then			display dialog "Configuration error : uncrustify could not be found at location '/usr/local/bin/uncrustify'. Please install uncrustify at this location."			return		end if				-- format Xcode opened files :		tell application "Xcode"						-- recover uncrustify configuration file :			tell active workspace document				set xcworkspaceFilePath to path				set subprojectPath to (do shell script "dirname " &amp; "'" &amp; xcworkspaceFilePath &amp; "'")				set uncrustifyConfigFilePath to (do shell script "find " &amp; "'" &amp; subprojectPath &amp; "'" &amp; " -name 'uncrustify_objective_c.cfg'")								tell application "System Events"										if not (exists file uncrustifyConfigFilePath) then						set projectPath to (do shell script "dirname " &amp; "'" &amp; subprojectPath &amp; "'")						set uncrustifyConfigFilePath to (do shell script "find " &amp; "'" &amp; projectPath &amp; "'" &amp; " -name 'uncrustify_objective_c.cfg'")					end if										-- if no local config file, use global one :					if not (exists file uncrustifyConfigFilePath) then						set uncrustifyConfigFilePath to (do shell script "dirname " &amp; "~/.uncrustify/uncrustify_objective_c.cfg")						set uncrustifyConfigFilePath to uncrustifyConfigFilePath &amp; "/uncrustify_objective_c.cfg"					end if									end tell			end tell						-- format all opened Xcode sources :			set openedDocumentsCount to count of source document			repeat with n from 1 to openedDocumentsCount								set currentDocument to source document [n]				set currentDocumentPath to path of currentDocument				set suffix to text (length of currentDocumentPath) thru -1 of currentDocumentPath								if (suffix = "m" or suffix = "h") then					set rawSource to quoted form of (text of currentDocument as Unicode text)					set formattedSource to do shell script "echo " &amp; rawSource &amp; " | /usr/local/bin/uncrustify -l OC -q -c " &amp; "'" &amp; uncrustifyConfigFilePath &amp; "'"					set text of currentDocument to formattedSource				end if							end repeat		end tell	end if	on error errorMessage	display dialog "Unknown error : " &amp; errorMessageend tryon isApplicationRunning(applicationName)	tell application "System Events" to (name of processes) contains applicationNameend isApplicationRunningon isUncrustifyCorrectlyInstalled()	tell application "System Events"		return (exists "/usr/local/bin/uncrustify")	end tellend isUncrustifyCorrectlyInstalled</string>
